generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Datum {
  id         Int     @id @default(autoincrement())
  uuid       String  @unique @db.VarChar(256)
  createdAt  String  @db.VarChar(256)
  modifiedAt String? @db.VarChar(256)
  userId     String? @db.VarChar(256)
}

model Tag {
  id        Int     @id @default(autoincrement())
  datumUuid String  @db.VarChar(256)
  name      String  @db.VarChar(255)
  value     String? @db.VarChar(255)
  unit      String? @db.VarChar(255)
  color     String  @db.VarChar(64)
}
